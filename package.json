{
  "name": "tzo",
  "version": "1.0.11",
  "description": "A simple Stack-based Virtual Machine",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "prepublishOnly": "npm run build",
    "build": "npx cross-env rm -rf dist/ && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && cp cjs_package.json dist/cjs/package.json && cp mjs_package.json dist/mjs/package.json",
    "start": "npm run build ; node ./dist/cjs/cli.js"
  },
  "bin": {
    "tzo": "./bin/tzo.mjs"
  },
  "author": "Joris van de Donk <jorisvddonk@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@types/unist": "^2.0.3",
    "commander": "^6.1.0",
    "debug": "^4.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "unist-builder": "^2.0.3"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/jest": "^26.0.14",
    "jest": "^26.4.2",
    "jest-mock-process": "^1.4.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.0"
  },
  "exports": {
    ".": "./dist/index.js",
    "./vm": "./dist/vm.js",
    "./getStackParams": "./dist/getStackParams.js"
  }
