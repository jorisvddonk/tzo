  "license": "MIT",
  "dependencies": {
    "@types/unist": "^2.0.3",
    "antlr4": "4.8",
    "commander": "^6.1.0",
    "debug": "^4.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "unist-builder": "^2.0.3"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/jest": "^26.0.14",
    "glob": "^7.2.0",
    "jest": "^26.4.2",
    "jest-mock-process": "^1.4.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.0"
  },
  "exports": {
    ".": {
      "import": "./dist/node/index.js",
      "require": "./dist/browser/index.js"
    },
    "./vm": {
