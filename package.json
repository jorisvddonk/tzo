{
  "name": "tzo",
  "version": "1.0.14",
  "description": "A simple Stack-based Virtual Machine",
  "main": "dist/mjs/index.js",
  "types": "dist/mjs/index.d.ts",
  "scripts": {
    "test": "jest",
    "prepublishOnly": "npm run build",
    "build": "npx cross-env rm -rf dist/ && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && cp cjs_package.json dist/cjs/package.json && cp mjs_package.json dist/mjs/package.json",
    "start": "npm run build ; node ./dist/cjs/cli.js",
    "antlr4ts-concisetext": "antlr4ts -visitor src/grammars/ConciseText.g4 -o ./src/concisetext",
    "fix_antlr_grammar_ts": "node src/util/fix_antlr_grammar_ts.js",
    "antlr4ts": "npm run antlr4ts-concisetext && npm run fix_antlr_grammar_ts",
    "ts-node": "ts-node"
  },
  "bin": {
    "tzo": "./bin/tzo.mjs"
  },
  "author": "Joris van de Donk <jorisvddonk@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@types/unist": "^2.0.3",
    "antlr4ts": "^0.5.0-alpha.4",
    "commander": "^6.1.0",
    "debug": "^4.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "unist-builder": "^2.0.3"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/jest": "^26.0.14",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "glob": "^7.2.0",
    "jest": "^26.4.2",
    "jest-mock-process": "^1.4.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.0"
  },
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./vm": {
      "import": "./dist/mjs/vm.js",
      "require": "./dist/cjs/vm.js"
    },
    "./vgetStackParamsm": {
      "import": "./dist/mjs/getStackParams.js",
      "require": "./dist/cjs/getStackParams.js"
    }
  }
}
